     type = "talker"
     name = "talker_modified"
     args= "$(arg frequencyInput)"
     launch-prefix="xterm -e"
   />
  <node
     pkg = "beginner_tutorials"
     type = "listener"
     name = "listener_modified"
     launch-prefix="xterm -e"
   />
</launch>
